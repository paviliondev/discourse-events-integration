<EventsIntegrationHeader @view={{view}} @message={{message}}></EventsIntegrationHeader>

<div class="admin-events-integration-container">
  {{#if hasLogs}}
    {{events-integration-log-table
      logs=logs
      page=page
      order=order
      asc=asc}}
  {{else}}
    <p>{{i18n "admin.events_integration.log.none"}}</p>
  {{/if}}
</div>
